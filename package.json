{
    "name": "add-to-snippet",
    "displayName": "Add-To-Snippet",
    "description": "Add select text to current language's snippet.",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.AddToSnippet"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.AddToSnippet",
                "title": "Add-To-Snippet"
            }
        ],
        "keybindings": [
            {
                "command": "extension.AddToSnippet",
                "key": "ctrl+i",
                "mac": "cmd+i",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^3.1.4",
        "vscode": "^1.1.25"
    },
    "dependencies": {}
}